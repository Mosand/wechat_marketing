<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="com.entity.TGF" table="tgf" entity-name="tgf">
           <id name="id" column="id">
                <generator class="increment"></generator>
           </id>
           <property name="goods_id" column="goods_id"></property>
           <property name="startNum" column="startNum"></property>
           <property name="tuiguangfei" column="tuiguangfei"></property>
           <property name="endNum" column="endNum"></property>
     </class>
</hibernate-mapping>