<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="com.entity.Address" table="address" entity-name="address">
           <id name="id" column="id">
                <generator class="increment"></generator>
           </id>
           <property name="id1" column="id1"></property>
           <property name="region" column="region"></property>
           <property name="tel" column="tel"></property>
           <property name="name" column="name"></property>
           <property name="first_choice" column="first_choice"></property>
           <property name="address" column="address"></property>        
     </class>
</hibernate-mapping>